{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/api/*": ["api/*"],
      "@/components/*": ["components/*"],
      "@/interfaces/*": ["interfaces/*"]
    },
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "strict": false,
    "strictBindCallApply": false, //should be true
    "strictFunctionTypes": false, //should be true
    "strictNullChecks": false, //should be true
    "noImplicitAny": false, //should be true
    "noImplicitReturns": false, //should be true
    "noImplicitThis": false, //should be true
    "noUnusedLocals": false, //should be true
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "noFallthroughCasesInSwitch": false, //should be true
    "sourceMap": true,
    "strictPropertyInitialization": false, //should be true
    "suppressImplicitAnyIndexErrors": false //should be true
  },
  "exclude": ["node_modules", ".next", "out", "_next"],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "next.config.js",
    "plopfile.mjs"
, "components/BarsChart.jsx"  ]
}
